<div class="super_container" >
	<!-- Deals of the week -->
	<div class="viewed 1">
		<div class="container">
			<div class="row">
				<div class="col">
					<div class="viewed_title_container">
						<h3 class="viewed_title" style="font-family: 'Lato', sans-serif;">Top 5 Sản Phẩm Gần Kết Thúc</h3>
						
					</div>

					<div class="viewed_slider_container">

						<!-- Recently Viewed Slider -->

						<div class="owl-carousel owl-theme viewed_slider">
							{{#each ProductsNearEnd}}
							
							<!-- Recently Viewed Item -->
							<div class="owl-item">
								<a href="/detail/{{MA_SAN_PHAM}}" >
								<div
									class="viewed_item discount d-flex flex-column align-items-center justify-content-center text-center">
									<div class="viewed_image"><img src="{{LINK_ANH}}" alt=""></div>
									<div class="viewed_content text-center" style="height: 250px;">
										<div class="viewed_name">{{TEN_SAN_PHAM}}</div>
										<div class="viewed_price">VNĐ <del>{{GIA_HIEN_TAI}}</del></div>
										<div class="viewed_price">VNĐ <mark>{{this.info.TopPrice}}</mark></div>
										<p class="card-text"><small class="text-muted"><mark>{{this.info.HO_TEN}}</mark> đang giữ giá đầu cao nhất</small></p>
										
									</div>
									<ul class="item_marks">
										<li class="item_mark item_discount">{{this.info.SL}} lượt</li>
										<li class="item_mark item_time">{{this.Time}}s</li>
										{{#if this.deadline}}
											{{{this.deadline}}}
										{{/if}}
									</ul>
									
								</div>
								
							</a>
							</div>
							{{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
